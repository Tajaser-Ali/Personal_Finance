<AnchorPane prefHeight="450.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="numberGuess.numberGuessController">
   <children>
      <VBox fx:id="vBox" layoutX="13.0" layoutY="97.0" prefHeight="200.0" prefWidth="173.0" />
      <TextField fx:id="txtGuess" layoutX="13.0" layoutY="61.0" prefHeight="26.0" prefWidth="173.0" text="Enter 2-Digit Number Guess" />
      <Button fx:id="btnCheck" layoutX="186.0" layoutY="61.0" mnemonicParsing="false" onAction="#checkGuess" text="Check" />
      <Label fx:id="lbAlert" layoutX="13.0" layoutY="314.0" prefHeight="115.0" prefWidth="236.0" />
   </children>
</AnchorPane>
